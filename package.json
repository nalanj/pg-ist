{
	"name": "pg-ist",
	"description": "Batteries included PostgreSQL Library for Node.js",
	"type": "module",
	"exports": {
		".": "./dist/index.js"
	},
	"version": "0.0.11",
	"scripts": {
		"fix": "biome check --write",
    "lint": "biome check",
		"test": "tsx --test",
		"citest": "biome check && tsx --test --test-reporter node-test-github-reporter",
		"build": "tsc",
		"clean": "rm -rf dist",
		"prepare": "rm -rf dist; tsc"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nalanj/pg-ist.git"
	},
	"keywords": [
		"utilities",
		"pg",
		"postgresql"
	],
	"author": "Alan Johnson",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/nalanj/pg-ist/issues"
	},
	"homepage": "https://github.com/nalanj/pg-ist#readme",
	"dependencies": {
		"pg": "8.14.1",
		"pg-cursor": "2.15.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "22.15.3",
		"@types/pg": "8.11.11",
		"@types/pg-cursor": "2.7.2",
		"node-test-github-reporter": "1.3.0",
		"tsx": "4.19.3",
		"typescript": "5.8.3"
	},
	"volta": {
		"node": "22.14.0"
	},
	"files": [
		"dist/**/*",
		"README.md",
		"LICENSE"
	]
}
