{
	"name": "pg-ist",
	"description": "PostgreSQL powered by template strings",
	"type": "module",
	"exports": {
		".": "./dist/index.js"
	},
	"version": "0.0.2",
	"scripts": {
		"fix": "biome check --write",
		"test": "biome check && tsx --test",
		"citest": "biome check && tsx --test --test-reporter node-test-github-reporter",
		"build": "tsc",
		"clean": "rm -rf dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nalanj/pg-ist.git"
	},
	"keywords": ["utilities", "pg", "postgresql"],
	"author": "Alan Johnson",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/nalanj/pg-ist/issues"
	},
	"homepage": "https://github.com/nalanj/pg-ist#readme",
	"dependencies": {
		"pg": "8.13.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "22.13.1",
		"@types/pg": "8.11.11",
		"node-test-github-reporter": "1.2.0",
		"tsx": "4.19.2",
		"typescript": "5.7.3"
	},
	"volta": {
		"node": "22.14.0"
	},
	"files": ["dist/**/*", "README.md", "LICENSE"]
}
